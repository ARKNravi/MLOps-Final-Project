{
    "version": 3,
    "functions": {
        "script/app.py": {
            "runtime": "@vercel/python@3.1.49",
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/script/app.py"
        }
    ],
    "buildCommand": "pip install -r requirements-deploy.txt"
}
