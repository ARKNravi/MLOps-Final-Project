{
    "version": 2,
    "builds": [
        {
            "src": "script/app.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9",
                "installCommand": "pip install -r requirements-deploy.txt --no-cache-dir",
                "buildCommand": "pip list"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "script/app.py"
        }
    ],
    "env": {
        "PYTHON_VERSION": "3.9",
        "PIP_TARGET": ".pip",
        "PYTHONPATH": "/var/task/.pip"
    }
}
